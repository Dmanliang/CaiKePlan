<?xml version="1.0" encoding="utf-8"?>
<shape xmlns:android="http://schemas.android.com/apk/res/android" >




    <!-- 间隔 -->

    <padding
        android:bottom="0dp"
        android:left="0dp"
        android:right="0dp"
        android:top="1dp" /><!-- 各方向的间隔 -->

    <!-- 大小 -->
    <size
        android:width="50dp"
        android:height="50dp"/><!-- 宽度和高度 -->

    <!-- 填充 -->
    <solid
        android:color="@color/whileItem"/><!-- 填充的颜色 -->

    <!-- 描边，边框 -->
    <stroke
        android:width="0.01dp"
        android:color="@color/grayDate"/>

</shape>